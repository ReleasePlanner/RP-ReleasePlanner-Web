{
  "name": "rp-release-planner",
  "version": "1.0.0",
  "private": true,
  "description": "Release Planner System - Monorepo with Web Portal, API, and Shared Libraries",
  "workspaces": [
    "apps/*",
    "libs/*",
    "libs/shared/*",
    "libs/api/*"
  ],
  "scripts": {
    "dev": "nx serve portal",
    "dev:api": "nx serve api",
    "dev:mobile": "nx start mobile",
    "dev:all": "nx run-many --target=serve --projects=portal,api",
    "build": "nx run-many --target=build --all",
    "build:portal": "nx build portal",
    "build:api": "nx build api",
    "test": "nx run-many --target=test --all",
    "test:portal": "nx test portal",
    "test:api": "nx test api",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "lint": "nx run-many --target=lint --all",
    "lint:portal": "nx lint portal",
    "lint:api": "nx lint api",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "graph": "nx graph",
    "affected:test": "nx affected --target=test",
    "affected:build": "nx affected --target=build",
    "affected:lint": "nx affected --target=lint"
  },
